[VARIABLE]

int: $age = 5
string: $name = "Ubert"
float: $money = 56.70

$age  >>  5
$name  >>  "Ubert"
$money  >>  56.70

type: $age  >> int
type: $name  >> string
type: $money  >>  float


[OPERATOR]

>> 1 + 4 - 3

operator = ['+', '-'] == a[1::2]
number = [1, 4, 3] == a[0::2]
result = 0
num = 0

for i in operator:
    if i == "+":
        result += number[num]

    ...

